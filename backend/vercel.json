{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "includeFiles": [
        "api/models/**/*",
        "api/config/**/*"
      ]
    }
  }
}